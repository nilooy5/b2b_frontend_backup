{
    "name": "shebab2bfrontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
        "start": "ng serve --port 3333 --project shebab2bfrontendDev",
        "dev": "ng serve --port 8084 --project shebab2bfrontendDev",
        "local": "ng serve --port 8084 --project local --configuration=production",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "analyze-webpack-bundle": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
        "build-with-source-map": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --prod --configuration=development --sourceMap --vendor-chunk=true",
        "source-map": "source-map-explorer",
        "compile:server": "webpack --config webpack.server.config.js --progress --colors",
        "serve:ssr": "node local.js",
        "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
        "build:client-and-server-bundles": "ng build --prod && ng run shebab2bfrontend:server:production",
        "server": "node local.js",
        "build:prod": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --prod --configuration=production --output-hashing=all",
        "build:dev": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --prod --configuration=development --output-hashing=all",
        "build:release": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --prod --configuration=release --output-hashing=all",
        "serve:prerender": "node static.js",
        "build:prerender": "npm run build:prod && node dist/prerender.js",
        "deploy": "gulp deploy"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/cdk": "~7.3.7",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "~7.2.0",
        "@angular/material": "^7.3.7",
        "@angular/material-moment-adapter": "^7.3.7",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/platform-server": "~7.2.0",
        "@angular/pwa": "^0.12.4",
        "@angular/router": "~7.2.0",
        "@angular/service-worker": "~7.2.0",
        "@kolkov/angular-editor": "^0.18.8",
        "@ng-bootstrap/ng-bootstrap": "^4.1.2",
        "@ng-select/ng-select": "^2.20.5",
        "@ng-toolkit/universal": "^7.1.1",
        "@nguniversal/common": "0.0.0",
        "@nguniversal/express-engine": "0.0.0",
        "@nguniversal/module-map-ngfactory-loader": "0.0.0",
        "@ngx-share/core": "^6.0.1",
        "@sentry/browser": "^5.12.5",
        "amplitude-js": "^5.7.0",
        "angular-calendar": "^0.27.7",
        "angular-file": "^1.2.1",
        "angular-resizable-element": "^3.2.4",
        "angular4-material-table": "0.7.0",
        "bootstrap": "^4.3.1",
        "core-js": "^2.5.4",
        "date-fns": "^1.30.1",
        "domino": "^2.1.3",
        "express": "^4.15.2",
        "hammerjs": "^2.0.8",
        "jquery": "^3.4.0",
        "lity": "^2.4.1",
        "magic-grid": "^3.2.4",
        "masonry-layout": "^4.2.2",
        "moment": "^2.24.0",
        "multiple-date-picker-angular": "^3.0.0",
        "ng-circle-progress": "^1.5.1",
        "ng-lazyload-image": "^5.1.2",
        "ng-pick-datetime": "^7.0.0",
        "ng2-file-upload": "^1.3.0",
        "ng5-slider": "^1.2.4",
        "ngx-bootstrap": "^5.0.0",
        "ngx-chips": "2.0.2",
        "ngx-daterangepicker-material": "2.1.10",
        "ngx-editor": "^4.1.0",
        "ngx-gallery": "^5.10.0",
        "ngx-masonry": "1.1.4",
        "ngx-slick-carousel": "^0.4.7",
        "ngx-summernote": "^0.6.5",
        "rxjs": "~6.3.3",
        "slick-carousel": "^1.8.1",
        "socket.io-client": "^2.3.0",
        "summernote": "^0.8.16",
        "sweetalert2": "^9.10.9",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.8",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/google.analytics": "0.0.40",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "concurrent-transform": "^1.0.0",
        "dotenv": "^8.1.0",
        "gulp": "^4.0.2",
        "gulp-awspublish": "^4.0.0",
        "gulp-clean-css": "^4.2.0",
        "gulp-gzip": "^1.4.2",
        "gulp-rename": "^1.4.0",
        "gulp-uglify-es": "^1.0.4",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "merge-stream": "^2.0.0",
        "node-sass": "^4.14.1",
        "protractor": "~5.4.0",
        "source-map-explorer": "2.4.2",
        "ts-loader": "^5.2.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2",
        "webpack-bundle-analyzer": "3.7.0",
        "webpack-cli": "^3.1.0"
    }
}
